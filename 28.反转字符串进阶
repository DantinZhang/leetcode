```javascript
/**
 * @param {string} s
 * @param {number} k
 * @return {string}
 */
var reverseStr = function(s, k) {
    let strArr = s.split('');
    for(let i = 0; i < strArr.length; i+=2*k) {
        let end = (i + k - 1 < s.length - 1) ? (i + k - 1) : (s.length - 1)
        reverse(strArr, i, end)
    }
    return strArr.join('')
};

const reverse = (strArr, left, right) => {
    while(left < right) {
        let temp = strArr[left];
        strArr[left] = strArr[right];
        strArr[right] = temp;
        left++;
        right--;
    }
}
```
